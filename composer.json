        "phroute/phroute": "^2.2",
        "php-di/php-di": "^7.0",
        "lion/dependency-injection": "^1.5"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.8",
        "lion/test": "^2.0",
        "phpunit/phpunit": "^11.1",
        "lion/exceptions": "^1.1",
        "robiningelbrecht/phpunit-pretty-print": "^1.3"
    }
}
