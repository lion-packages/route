{
    "name": "lion/route",
    "description": "This library has quick router usage with regular expressions.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lion\\Route\\": "src/LionRoute/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.4",
        "lion/phroute": "^2.3"
    },
    "require-dev": {
        "lion/test": "^3.2",
        "guzzlehttp/guzzle": "^7.9",
        "lion/security": "^11.1",
        "lion/request": "^7.2",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.11 || ^4.0",
        "lion/dependency-injection": "^4.2",
        "lion/exceptions": "^2.0",
        "phpunit/phpunit": "^12.0",
        "overtrue/phplint": "^9.5",
        "infection/infection": "^0.29.14 || ^0.31.0"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": false
        }
    }
}
